<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity" >

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="409dp"
        android:layout_height="729dp"
        android:background="@color/background"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0">


        <TextView
            android:id="@+id/textView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="none"
            android:layout_marginBottom="84dp"
            android:text="Alarm"
            android:textSize="20sp"
            app:layout_constraintBottom_toTopOf="@+id/linearLayout"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

        <ImageButton
            android:id="@+id/menubutton"
            android:layout_width="32dp"
            android:layout_height="20dp"
            android:layout_marginStart="52dp"
            android:layout_marginTop="116dp"
            android:contentDescription="예제 이미지"
            android:onClick="MenuButton"
            app:layout_constraintBottom_toTopOf="@id/linearLayout"
            app:layout_constraintEnd_toStartOf="@id/textView2"
            app:layout_constraintHorizontal_bias="0.029"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.03"
            app:srcCompat="@android:drawable/ic_menu_sort_by_size" />

        <ImageButton
            android:id="@+id/MyPageButton"
            android:layout_width="27dp"
            android:layout_height="27dp"
            android:layout_marginTop="112dp"
            android:layout_marginEnd="43dp"
            android:contentDescription="예제 이미지"
            android:onClick="MyPageButton"
            app:layout_constraintBottom_toTopOf="@+id/linearLayout"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toEndOf="@+id/textView2"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0"
            tools:srcCompat="@tools:sample/avatars" />

        <ImageButton
            android:id="@+id/AddButton"
            android:layout_width="27dp"
            android:layout_height="27dp"
            android:layout_marginTop="30dp"
            android:layout_marginEnd="43dp"
            android:contentDescription="예제 이미지"
            android:onClick="AddButton"
            app:layout_constraintBottom_toTopOf="@+id/linearLayout"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toEndOf="@+id/textView2"
            app:layout_constraintTop_toBottomOf="@+id/MyPageButton"
            app:layout_constraintVertical_bias="0.025"
            tools:srcCompat="@tools:sample/avatars" />

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="32dp"
            android:layout_height="32dp"
            android:layout_marginStart="56dp"
            android:layout_marginTop="26dp"
            android:layout_marginEnd="251dp"
            android:layout_marginBottom="22dp"
            app:layout_constraintBottom_toTopOf="@+id/linearLayout"
            app:layout_constraintEnd_toStartOf="@+id/AddButton"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/menubutton"
            app:srcCompat="@drawable/ic_launcher_foreground" />

        <ImageView
            android:id="@+id/logo"
            android:layout_width="151dp"
            android:layout_height="97dp"
            android:layout_marginStart="129dp"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="129dp"
            android:layout_marginBottom="3dp"
            app:layout_constraintBottom_toTopOf="@+id/textView2"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/logo" />

        <LinearLayout
            android:id="@+id/linearLayout"
            android:layout_width="407dp"
            android:layout_height="510dp"
            android:orientation="vertical"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent">

            <LinearLayout
                android:layout_width="333dp"
                android:layout_height="match_parent"
                android:layout_marginStart="38dp"
                android:orientation="horizontal">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="100"
                    android:orientation="vertical">

                    <FrameLayout
                        android:id="@+id/alarm1"
                        android:layout_width="match_parent"
                        android:layout_height="77dp">

                        <Switch
                            android:id="@+id/switch1"
                            android:layout_width="match_parent"
                            android:layout_height="65dp"
                            android:background="@drawable/buttonradius" />

                        <Button
                            android:id="@+id/button1"
                            android:layout_width="276dp"
                            android:layout_height="65dp"
                            android:layout_marginBottom="10dp"
                            android:background="#00ff0000"
                            android:visibility="visible" />

                        <EditText
                            android:id="@+id/editTextTime"
                            android:layout_width="123dp"
                            android:layout_height="65dp"
                            android:background="@null"
                            android:clickable="false"
                            android:ems="10"
                            android:focusable="false"
                            android:inputType="time"
                            tools:ignore="MissingConstraints" />

                    </FrameLayout>

                    <FrameLayout
                        android:id="@+id/alarm2"
                        android:layout_width="332dp"
                        android:layout_height="77dp"
                        android:layout_marginTop="5dp">

                        <Switch
                            android:id="@+id/switch2"
                            android:layout_width="match_parent"
                            android:layout_height="65dp"
                            android:background="@drawable/buttonradius" />

                        <Button
                            android:id="@+id/button2"
                            android:layout_width="276dp"
                            android:layout_height="65dp"
                            android:layout_marginBottom="10dp"
                            android:background="#00ff0000" />

                        <EditText
                            android:id="@+id/editTextTime2"
                            android:layout_width="123dp"
                            android:layout_height="65dp"
                            android:clickable="false"
                            android:ems="10"
                            android:focusable="false"
                            android:inputType="time"
                            android:background="@null"
                            tools:ignore="MissingConstraints" />
                    </FrameLayout>

                    <FrameLayout
                        android:id="@+id/alarm3"
                        android:layout_width="match_parent"
                        android:layout_height="77dp">

                        <Switch
                            android:id="@+id/switch3"
                            android:layout_width="match_parent"
                            android:layout_height="65dp"
                            android:background="@drawable/buttonradius" />

                        <Button
                            android:id="@+id/button3"
                            android:layout_width="276dp"
                            android:layout_height="65dp"
                            android:layout_marginBottom="10dp"
                            android:background="#00ff0000" />

                        <EditText
                            android:id="@+id/editTextTime3"
                            android:layout_width="123dp"
                            android:layout_height="65dp"
                            android:clickable="false"
                            android:ems="10"
                            android:focusable="false"
                            android:inputType="time"
                            android:background="@null"
                            tools:ignore="MissingConstraints" />

                    </FrameLayout>

                    <FrameLayout
                        android:id="@+id/alarm4"
                        android:layout_width="match_parent"
                        android:layout_height="77dp">

                        <Switch
                            android:id="@+id/switch4"
                            android:layout_width="match_parent"
                            android:layout_height="65dp"
                            android:background="@drawable/buttonradius" />

                        <Button
                            android:id="@+id/button4"
                            android:layout_width="276dp"
                            android:layout_height="65dp"
                            android:layout_marginBottom="10dp"
                            android:background="#00ff0000" />

                        <EditText
                            android:id="@+id/editTextTime4"
                            android:layout_width="123dp"
                            android:layout_height="65dp"
                            android:clickable="false"
                            android:ems="10"
                            android:focusable="false"
                            android:background="@null"
                            android:inputType="time"
                            tools:ignore="MissingConstraints" />

                    </FrameLayout>

                    <FrameLayout
                        android:id="@+id/alarm5"
                        android:layout_width="match_parent"
                        android:layout_height="77dp">

                        <Switch
                            android:id="@+id/switch5"
                            android:layout_width="match_parent"
                            android:layout_height="65dp"
                            android:background="@drawable/buttonradius" />

                        <Button
                            android:id="@+id/button5"
                            android:layout_width="276dp"
                            android:layout_height="65dp"
                            android:layout_marginBottom="10dp"
                            android:background="#00ff0000" />

                        <EditText
                            android:id="@+id/editTextTime5"
                            android:layout_width="123dp"
                            android:layout_height="65dp"
                            android:clickable="false"
                            android:ems="10"
                            android:focusable="false"
                            android:inputType="time"
                            android:background="@null"
                            tools:ignore="MissingConstraints" />

                    </FrameLayout>

                </LinearLayout>
            </LinearLayout>

        </LinearLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>